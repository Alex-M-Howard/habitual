{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "date": {
      "type": "string",
      "format": "date-time",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
    },
    "entry": {
      "type": "string",
      "maxLength": 5000
    }
  },
  "additionalProperties": true,
  "required": [
    "entry"
  ]
}
